<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Hello</servlet-name>
  	<display-name>Hello</display-name>
  	<description></description>
  	<servlet-class>controller.Hello</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>HelloServlet</servlet-name>
  	<display-name>HelloServlet</display-name>
  	<description></description>
  	<servlet-class>controller.HelloServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>HomeServlet</servlet-name>
  	<display-name>HomeServlet</display-name>
  	<description></description>
  	<servlet-class>controller.HomeServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SuccessForm</servlet-name>
  	<display-name>SuccessForm</display-name>
  	<description></description>
  	<servlet-class>controller.SuccessForm</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ChuNhatServlet</servlet-name>
  	<display-name>ChuNhatServlet</display-name>
  	<description></description>
  	<servlet-class>controller.ChuNhatServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Hello</servlet-name>
  	<url-pattern>/Hello</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>HelloServlet</servlet-name>
  	<url-pattern>/hello-servlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>HomeServlet</servlet-name>
  	<url-pattern>/home-servlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SuccessForm</servlet-name>
  	<url-pattern>/success-form</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ChuNhatServlet</servlet-name>
  	<url-pattern>/chu-nhat-servlet</url-pattern>
  </servlet-mapping>
</web-app>
